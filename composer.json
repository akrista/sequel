{
    "name": "akrista/sequel",
    "description": "Clear and concise database management.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Quinten Justus",
            "email": "quinten@protoqol.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1|^8.5",
        "laravel/framework": "^11.0|^12.0",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akrista\\Sequel\\SequelServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpmd/phpmd": "^2.6",
        "rector/rector": "^2.3",
        "laravel/pint": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "Akrista\\Sequel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akrista\\Sequel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpins": [
            "./vendor/bin/phpinsights"
        ],
        "phpmd": [
            "phpmd src html cleancode,codesize,controversial,design,naming,unusedcode > public/phpmd.html"
        ],
        "test": [
            "phpunit"
        ],
        "format": ["pint --ansi"],
        "lint": [
            "rector"
        ]
    },
    "scripts-descriptions": {
        "phpins": "Runs nunomaduro's PHPInsights. This gives you a report about the current code quality.",
        "test": "Runs the default PHPUnit test suite configured in phpunit.xml"
    }
}
